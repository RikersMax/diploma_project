<%= provide(:errors_messages) do  %>

	<% if object_error.errors.full_messages then %>	
		<div class="message-main-container">
			<div class="message-container">
				<ul class="list-errors">						
					<% object_error.errors.full_messages.each do |msg| %>
						<li><%= msg %></li>
					<% end %>		
				</ul>
			</div>
		</div>
	<% end %>

<% end %>
